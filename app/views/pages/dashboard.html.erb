<div class="row">
  <div class="small-6 columns">
    <div class='load-catalog'>
      <%= render 'wine_dot_com/load_catalog_form' %>
    </div>
  </div>
  <div class="small-6 columns">
    <div class='dump-catalog'>
      <%= link_to 'Remove Wine.com data', dump_catalog_path, method: :delete, remote: true, data: { confirm: 'Are you sure? This cannot be undone.' }, class: 'button' %>
    </div>
  </div>
</div>

<div class="row">
  <div class="small-6 columns">
    <div class="spinner">
      <div class="rect1"></div>
      <div class="rect2"></div>
      <div class="rect3"></div>
      <div class="rect4"></div>
      <div class="rect5"></div>
    </div>
  </div>
</div>
